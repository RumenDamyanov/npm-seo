{
  "name": "@rumenx/seo",
  "version": "1.0.1",
  "description": "A comprehensive TypeScript package for AI-powered SEO optimization in Node.js applications. Framework-agnostic with built-in AI integration, rich meta tag generation, and 100% type safety.",
  "keywords": [
    "seo",
    "ai",
    "meta-tags",
    "typescript",
    "nodejs",
    "express",
    "nextjs",
    "fastify",
    "openai",
    "claude",
    "gemini",
    "seo-optimization",
    "ai-powered",
    "framework-agnostic"
  ],
  "author": {
    "name": "Rumen Damyanov",
    "email": "contact@rumenx.com",
    "url": "https://github.com/RumenDamyanov"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/RumenDamyanov/npm-seo.git"
  },
  "bugs": {
    "url": "https://github.com/RumenDamyanov/npm-seo/issues"
  },
  "homepage": "https://github.com/RumenDamyanov/npm-seo#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/RumenDamyanov"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:esm && npm run build:cjs && npm run fix-extensions",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rimraf dist coverage",
    "dev": "tsc -p tsconfig.esm.json --watch",
    "fix-extensions": "node scripts/fix-cjs-extensions.mjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage-html": "jest --coverage --coverageReporters=html",
    "lint": "eslint src tests examples",
    "lint:fix": "eslint src tests examples --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\" \"*.{js,json,md}\"",
    "typecheck": "tsc --noEmit",
    "quality": "npm run lint && npm run format:check && npm run typecheck",
    "prepublishOnly": "npm run quality && npm run test && npm run build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "jsdom": "^27.0.0",
    "node-html-parser": "^7.0.1",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^24.5.2",
    "@types/sanitize-html": "^2.16.0",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.1.3",
    "fastify": "^5.6.1",
    "jest": "^30.2.0",
    "next": "^15.5.4",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.9",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.20.0",
    "@google/generative-ai": "^0.2.0",
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "@google/generative-ai": {
      "optional": true
    }
  }
}
